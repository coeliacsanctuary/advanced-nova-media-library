{
    "name": "jpeters8889/advanced-nova-media-library",
    "description": "Laravel Nova tools for managing the Spatie media library.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "laravel/framework": "^11.0|^12.0",
        "laravel/nova": "^5.0",
        "spatie/data-transfer-object": "^3.9",
        "spatie/laravel-medialibrary": "^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Jpeters8889\\AdvancedNovaMediaLibrary\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
